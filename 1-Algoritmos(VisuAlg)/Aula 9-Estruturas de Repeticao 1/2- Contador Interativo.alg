algoritmo "Contador Interativo"
var
   Cont,Valor,Salto,Maior,Menor: Inteiro

inicio
      Cont <-0
      Escreval ("Você quer a contagem até quanto ? ")
      Leia (Valor)
      Escreval ("Qual o valor do salto que você deseja? ")
      Leia (Salto)
      
      Enquanto (Cont <= valor) faca
               Escreval (Cont)
               Cont <- Cont + Salto
      FimEnquanto

fimalgoritmo
