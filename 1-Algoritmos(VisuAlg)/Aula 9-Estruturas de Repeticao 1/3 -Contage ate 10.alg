algoritmo "Contar até 10"
var
   cont:inteiro

inicio
      cont <-0
      Enquanto (cont <=10) faca
               Escreval (cont)
               cont <- cont + 1

      FimEnquanto
      
      Escreval ("---------------------")
      Escreval (" Terminei a contagem ")
      Escreval ("---------------------")
fimalgoritmo
