algoritmo "Teste de Vetor"
var
   Val:vetor [1..6] de Inteiro
   Cont:Inteiro

inicio
      Para Cont <- 1 ate 6 faca
           Escreva ("Digite o ",Cont,"º valor ==>")
           Leia (Val[Cont])
      FimPara

      Para Cont <- 1 ate 6 faca
           Escreval ("Na posição ",Cont," está o valo:",Val[Cont])
      FimPara
fimalgoritmo
