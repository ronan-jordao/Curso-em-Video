algoritmo "Qual a minha idade"
var
   ano_atual,ano_nasc,idade:inteiro

inicio
      Escreva ("Em que ano nós estamos ? ")
      Leia(ano_atual)
      Escreva ("Qual o ano em que nasci ? ")
      Leia(ano_nasc)
                      idade <- ano_atual-ano_nasc
      Escreva ("A minha idade é: ",idade)



fimalgoritmo
